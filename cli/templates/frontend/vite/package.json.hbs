{
  "name": "{{projectName}}-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "{{#if (eq frontendLanguage 'TypeScript')}}tsc -b && {{/if}}vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "{{#if (eq frontendLanguage 'TypeScript')}}tsc --noEmit{{else}}echo 'No type checking for JavaScript'{{/if}}"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0"{{#if includeWebsockets}},
    "socket.io-client": "^4.6.1"{{/if}}
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.1.0",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"{{#if (eq frontendLanguage 'TypeScript')}},
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3"{{/if}}
  }
}
